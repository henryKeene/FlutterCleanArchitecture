    "version": "0.2.0",
    "configurations": [
        {
            "name": "starter_app (development)",
            "cwd": "packages/app/starter_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "development",
                "--target",
                "lib/main_development.dart",
            ],
        },
        {
            "name": "starter_app (profile mode)",
            "cwd": "packages/app/starter_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "starter_app (release mode)",
            "cwd": "packages/app/starter_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
    ]
}